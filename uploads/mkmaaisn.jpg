<!DOCTYPE html>
<html lang="id">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>CloudkuImages - Modern Profile</title>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css">
<style>:root{--color-primary:#40e0d0;--color-secondary:#8a2be2;--color-tertiary:#4169e1;--bg-dark:#0d1117;--bg-card:rgba(21,32,43,.75);--text-primary:#f0f6fc;--text-secondary:#8b949e;--border-color:rgba(64,224,208,.15);--transition-smooth:all .4s cubic-bezier(.4,0,.2,1)}*{margin:0;padding:0;box-sizing:border-box}body{font-family:'Inter',sans-serif;background-color:var(--bg-dark);color:var(--text-primary);min-height:100vh;overflow-x:hidden;line-height:1.6}.bg-container{position:fixed;top:0;left:0;width:100%;height:100%;z-index:-1;will-change:transform}.bg-layer{width:100%;height:100%;background:radial-gradient(ellipse at 25% 30%,rgba(64,224,208,.1),transparent 60%) , radial-gradient(ellipse at 75% 70%,rgba(138,43,226,.08),transparent 60%);animation:bgFloat 20s ease-in-out infinite}@keyframes bgFloat {
            0%, 100% { transform: rotate(0deg) scale(1); }
            50% { transform: rotate(2deg) scale(1.03); }
        }.container{max-width:1200px;margin:0 auto;padding:20px;animation:slideUp 1s cubic-bezier(.4,0,.2,1)}@keyframes slideUp {
            from { opacity: 0; transform: translateY(25px); }
            to { opacity: 1; transform: translateY(0); }
        }.header{text-align:center;padding:40px 0 30px}.title{font-size:clamp(2.2rem,5vw,3.5rem);font-weight:800;background:linear-gradient(135deg,var(--color-primary),var(--color-secondary),var(--color-tertiary),var(--color-primary));background-size:300% 100%;-webkit-background-clip:text;-webkit-text-fill-color:transparent;background-clip:text;margin-bottom:8px;animation:gradientShift 4s ease-in-out infinite , headerGlow 3s ease-in-out infinite alternate}@keyframes gradientShift {
            0%, 100% { background-position: 0% 50%; }
            50% { background-position: 100% 50%; }
        }@keyframes headerGlow {
            from { filter: drop-shadow(0 0 8px rgba(64, 224, 208, 0.3)); }
            to { filter: drop-shadow(0 0 20px rgba(138, 43, 226, 0.4)); }
        }.subtitle{font-size:clamp(.9rem,2vw,1.1rem);color:var(--text-secondary);font-weight:400;opacity:0;animation:fadeIn 1s ease-out .5s forwards}@keyframes fadeIn { to { opacity: 1; } }.content-grid{display:grid;grid-template-columns:1fr 1fr;gap:20px;margin-bottom:25px}.card{background:var(--bg-card);backdrop-filter:blur(20px);border:1px solid var(--border-color);border-radius:20px;padding:24px;transition:var(--transition-smooth);will-change:transform , border-color , box-shadow;position:relative;overflow:hidden}.card::before{content:'';position:absolute;top:0;left:-100%;width:100%;height:100%;background:linear-gradient(90deg,transparent,rgba(64,224,208,.05),transparent);transition:left .6s ease;z-index:0}.card:hover::before{left:100%}.card:hover{transform:translateY(-5px) scale(1.02);border-color:rgba(64,224,208,.4);box-shadow:0 20px 40px rgba(0,0,0,.2)}.card>*{position:relative;z-index:1}.profile-card{text-align:center}.profile-img-container{position:relative;width:130px;height:130px;margin:0 auto 18px;cursor:pointer;border-radius:50%;background:linear-gradient(45deg,var(--color-primary),var(--color-secondary));transition:var(--transition-smooth);will-change:transform}.profile-img-container.animate{animation:profileClickAnim .8s cubic-bezier(.25,1,.5,1)}@keyframes profileClickAnim {
            0% { transform: scale(1) rotate(0deg); }
            50% { transform: scale(1.2) rotate(180deg); filter: drop-shadow(0 0 20px var(--color-primary)); }
            100% { transform: scale(1) rotate(360deg); filter: drop-shadow(0 0 0 transparent); }
        }.profile-img{width:calc(100% - 6px);height:calc(100% - 6px);border-radius:50%;object-fit:cover;transition:var(--transition-smooth);margin:3px;background:var(--bg-dark)}.profile-img-container:hover .profile-img{transform:scale(1.1)}.name{font-size:1.8rem;font-weight:700;background:linear-gradient(135deg,var(--color-primary),var(--color-secondary));-webkit-background-clip:text;-webkit-text-fill-color:transparent;background-clip:text;margin-bottom:8px}.role{color:var(--text-secondary);margin-bottom:12px;font-size:.95rem}.status{display:inline-flex;align-items:center;gap:8px;background:rgba(46,160,67,.2);border:1px solid rgba(46,160,67,.4);color:#3fb950;padding:8px 14px;border-radius:20px;font-size:.85rem;backdrop-filter:blur(12px);animation:statusFloat 3s ease-in-out infinite}@keyframes statusFloat {
            50% { transform: translateY(-3px); }
        }.status-dot{width:8px;height:8px;border-radius:50%;background:#3fb950;animation:pulse 1.5s infinite;box-shadow:0 0 8px rgba(63,185,80,.6)}@keyframes pulse {
            50% { opacity: 0.5; }
        }.bio-card h3{color:var(--color-primary);margin-bottom:12px;font-size:1.2rem;font-weight:600}.bio-card p{color:var(--text-primary);font-size:.95rem}.card.animated{opacity:0;transform:translateY(20px);animation:cardSlideIn .6s cubic-bezier(.4,0,.2,1) forwards;animation-delay: calc(var(--delay) * 0.1s)}@keyframes cardSlideIn {
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }.video-container{position:relative;width:100%;padding-top:56.25%;border-radius:16px;overflow:hidden;background-color:#000;cursor:pointer;transition:var(--transition-smooth)}.video-thumbnail,.video-player{position:absolute;top:0;left:0;width:100%;height:100%;object-fit:cover;transition:opacity .4s ease}.video-thumbnail.hidden{opacity:0;pointer-events:none}.video-overlay{position:absolute;inset:0;background:rgba(0,0,0,.3);display:flex;align-items:center;justify-content:center;opacity:1;transition:opacity .3s ease;backdrop-filter:blur(2px)}.video-container.playing .video-overlay{opacity:0}.video-container:hover .video-overlay:not(.playing .video-overlay) {background:rgba(0,0,0,.5)}.play-btn{width:60px;height:60px;border-radius:50%;background:linear-gradient(135deg,var(--color-primary),var(--color-secondary));border:none;color:#fff;cursor:pointer;transition:var(--transition-smooth);display:flex;align-items:center;justify-content:center;font-size:1.4rem;box-shadow:0 8px 20px rgba(64,224,208,.2)}.play-btn:hover{transform:scale(1.1);box-shadow:0 12px 30px rgba(64,224,208,.3)}.music-card{display:flex;align-items:center;gap:20px}.music-info{flex:1}.music-title{font-weight:600;font-size:1rem;color:var(--color-primary)}.music-artist{color:var(--text-secondary);margin-bottom:10px;font-size:.9rem}.progress-bar{width:100%;height:6px;background:rgba(255,255,255,.1);border-radius:3px;overflow:hidden}.progress{height:100%;background:linear-gradient(90deg,var(--color-primary),var(--color-secondary));width:0%;transition:width .1s linear;border-radius:3px}.websites-grid,.social-grid{display:grid;gap:15px}.websites-grid{grid-template-columns:repeat(auto-fit,minmax(240px,1fr));margin-bottom:25px}.social-grid{grid-template-columns:repeat(auto-fit,minmax(150px,1fr))}.website-card,.social-link{background:var(--bg-card);backdrop-filter:blur(16px);border:1px solid var(--border-color);border-radius:16px;padding:24px;text-decoration:none;color:var(--text-primary);transition:var(--transition-smooth);display:block;text-align:center;position:relative;overflow:hidden}.website-card:hover,.social-link:hover{transform:translateY(-6px);border-color:rgba(64,224,208,.4);box-shadow:0 20px 40px rgba(0,0,0,.2)}.website-icon,.social-icon{font-size:2.2rem;margin-bottom:12px;display:block;transition:transform .3s ease;color:var(--color-primary)}.website-card:hover .website-icon,.social-link:hover .social-icon{transform:scale(1.15) rotate(-5deg)}.website-name{font-size:1.1rem;font-weight:600;margin-bottom:4px}.website-desc{font-size:.9rem;color:var(--text-secondary)}.social-name{font-size:1rem;font-weight:500}.social-link.github .social-icon{color:#f0f6fc}.social-link.whatsapp .social-icon{color:#25d366}.social-link.telegram .social-icon{color:#08c}.social-link.channel .social-icon{color:#ffa500}.social-link.twitter .social-icon{color:#1da1f2}@media (max-width:768px){.content-grid{grid-template-columns:1fr;gap:15px}.profile-img-container{width:110px;height:110px}.name{font-size:1.6rem}.music-card{flex-direction:column;text-align:center;gap:15px}.websites-grid,.social-grid{grid-template-columns:1fr 1fr}.card{padding:20px}}@media (max-width:480px){body{line-height:1.5}.container{padding:15px}.card{padding:16px}.websites-grid,.social-grid{grid-template-columns:1fr}.profile-img-container{width:100px;height:100px}.name{font-size:1.4rem}.play-btn{width:50px;height:50px;font-size:1.2rem}}@media (prefers-reduced-motion:reduce){*,*::before,*::after{animation-duration:.01ms!important;animation-iteration-count:1!important;transition-duration:.01ms!important;scroll-behavior:auto!important}}</style>
</head>
<body>
<div class="bg-container">
<div class="bg-layer"></div>
</div>
<div class="container">
<header class="header">
<h1 class="title">CloudkuImages</h1>
<p class="subtitle">Professional Developer & Digital Creator</p>
</header>
<div class="content-grid">
<div class="card profile-card animated" style="--delay: 0;">
<div id="profileImageContainer" class="profile-img-container">
<img src="https://cloudkuimages.guru/uploads/images/JxuXw0ca.png" alt="AlfiDev" class="profile-img">
</div>
<h2 class="name">AlfiDev</h2>
<p class="role">Newbie In Coding But I Love It</p>
<div class="status">
<div class="status-dot"></div>
Ready to Code
</div>
</div>
<div class="card bio-card animated" style="--delay: 0.1;">
<h3>About Me</h3>
<p>Seorang Pemula Dalam Development Dan Coding tapi aku suka itu karena bisa belajar untuk meningkatkan Skill Ku dalam bidang IT stay Up and Growth</p>
</div>
</div>
<div class="card animated" style="--delay: 0.2;">
<div id="videoContainer" class="video-container">
<img src="https://cloudkuimages.guru/uploads/images/JxuXw0ca.png" alt="Video Thumbnail" class="video-thumbnail" id="videoThumbnail">
<video class="video-player" id="videoPlayer" muted preload="none" playsinline>
<source src="https://cloudkuimages.guru/uploads/videos/6E7ptX0O.mp4" type="video/mp4">
</video>
<div class="video-overlay">
<button class="play-btn" aria-label="Play Video">
<i id="videoPlayIcon" class="fas fa-play"></i>
</button>
</div>
</div>
</div>
<div style="height: 20px;"></div>
<div class="card music-card animated" style="--delay: 0.3;">
<button id="musicToggleButton" class="play-btn" aria-label="Play Music">
<i class="fas fa-play" id="musicPlayIcon"></i>
</button>
<div class="music-info">
<div class="music-title">Nan Ko Paham</div>
<div class="music-artist">CloudkuImages</div>
<div class="progress-bar">
<div class="progress" id="musicProgress"></div>
</div>
</div>
</div>
<div style="height: 25px;"></div>
<div class="websites-grid">
<a href="https://cloudku.us.kg" class="website-card animated" target="_blank" style="--delay: 0.4;">
<i class="fas fa-code website-icon"></i>
<div class="website-name">cloudku.us.kg</div>
<div class="website-desc">API MUROTAL</div>
</a>
<a href="https://cloudkuimages.com" class="website-card animated" target="_blank" style="--delay: 0.5;">
<i class="fas fa-cloud website-icon"></i>
<div class="website-name">cloudkuimages.com</div>
<div class="website-desc">Mirror</div>
</a>
</div>
<div class="social-grid">
<a href="https://github.com/cloudkuimages" class="social-link github animated" target="_blank" style="--delay: 0.6;"><i class="fab fa-github social-icon"></i><span class="social-name">GitHub</span></a>
<a href="https://wa.me/6287831816747" class="social-link whatsapp animated" target="_blank" style="--delay: 0.7;"><i class="fab fa-whatsapp social-icon"></i><span class="social-name">WhatsApp</span></a>
<a href="https://t.me/CloudkuDev" class="social-link telegram animated" target="_blank" style="--delay: 0.8;"><i class="fab fa-telegram social-icon"></i><span class="social-name">Telegram</span></a>
<a href="https://www.whatsapp.com/channel/0029VasizxI47XeE2iiave0u" class="social-link channel animated" target="_blank" style="--delay: 0.9;"><i class="fas fa-bullhorn social-icon"></i><span class="social-name">WA Channel</span></a>
<a href="https://x.com/cloudkuimages" class="social-link twitter animated" target="_blank" style="--delay: 1;"><i class="fab fa-x-twitter social-icon"></i><span class="social-name">Twitter/X</span></a>
</div>
</div>
<audio id="musicAudio" preload="none">
<source src="https://cloudkuimages.guru/uploads/NanKoPaham.mp3" type="audio/mpeg">
</audio>
<script>
        document.addEventListener('DOMContentLoaded', () => {
            const musicAudio = document.getElementById('musicAudio');
            const videoPlayer = document.getElementById('videoPlayer');
            
            const musicToggleButton = document.getElementById('musicToggleButton');
            const musicPlayIcon = document.getElementById('musicPlayIcon');
            const musicProgress = document.getElementById('musicProgress');

            const videoContainer = document.getElementById('videoContainer');
            const videoPlayIcon = document.getElementById('videoPlayIcon');
            const videoThumbnail = document.getElementById('videoThumbnail');

            const profileImageContainer = document.getElementById('profileImageContainer');

            let isMusicPlaying = false;
            let isVideoPlaying = false;
            let isVideoLoaded = false;

            const toggleMusic = () => {
                if (isMusicPlaying) {
                    musicAudio.pause();
                } else {
                    if (isVideoPlaying) {
                        videoPlayer.pause();
                    }
                    musicAudio.play().catch(e => console.error("Audio play failed:", e));
                }
            };

            const toggleVideo = () => {
                if (!isVideoLoaded) {
                    videoPlayer.load();
                    videoThumbnail.classList.add('hidden');
                    isVideoLoaded = true;
                }
                
                if (isVideoPlaying) {
                    videoPlayer.pause();
                } else {
                    if (isMusicPlaying) {
                        musicAudio.pause();
                    }
                    videoPlayer.play().catch(e => console.error("Video play failed:", e));
                }
            };
            
            musicAudio.addEventListener('play', () => {
                isMusicPlaying = true;
                musicPlayIcon.className = 'fas fa-pause';
            });

            musicAudio.addEventListener('pause', () => {
                isMusicPlaying = false;
                musicPlayIcon.className = 'fas fa-play';
            });
            
            musicAudio.addEventListener('ended', () => {
                isMusicPlaying = false;
                musicPlayIcon.className = 'fas fa-play';
                musicProgress.style.width = '0%';
            });

            musicAudio.addEventListener('timeupdate', () => {
                if (musicAudio.duration) {
                    const progressPercent = (musicAudio.currentTime / musicAudio.duration) * 100;
                    musicProgress.style.width = `${progressPercent}%`;
                }
            });
            
            videoPlayer.addEventListener('play', () => {
                isVideoPlaying = true;
                videoPlayIcon.className = 'fas fa-pause';
                videoContainer.classList.add('playing');
            });
            
            videoPlayer.addEventListener('pause', () => {
                isVideoPlaying = false;
                videoPlayIcon.className = 'fas fa-play';
                videoContainer.classList.remove('playing');
            });
            
            videoPlayer.addEventListener('ended', () => {
                 isVideoPlaying = false;
                 videoPlayIcon.className = 'fas fa-play';
                 videoContainer.classList.remove('playing');
            });

            musicToggleButton.addEventListener('click', toggleMusic);
            videoContainer.addEventListener('click', toggleVideo);
            
            profileImageContainer.addEventListener('click', () => {
                if (!profileImageContainer.classList.contains('animate')) {
                    profileImageContainer.classList.add('animate');
                }
            });

            profileImageContainer.addEventListener('animationend', () => {
                profileImageContainer.classList.remove('animate');
            });

            videoPlayer.addEventListener('contextmenu', (e) => e.preventDefault());
        });
    </script>
</body>
</html>